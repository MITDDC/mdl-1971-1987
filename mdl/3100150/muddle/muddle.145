;MACRO TO CHECK FOR ATOM DUPLICATION AND CREATE UNIQUE ONE

UNATS==0	;NUMBER OF UNIQUE ATOMS
NATOM==0

;INITIALIZE ADRES

DEFINE ADRES
	%A0 TERMIN
DEFINE CHATOM NAME,TYPE,WHERE,VALU,PNAME,\ZZZ,FLAG,NTYP,NVAL

	CHSYMB [NAME]FLAG

	IFE FLAG,[	REPEAT UNATS,[EXEC MAC,\.RPCNT,[NAME]
	]

	DEFMAC MAC,\UNATS,[NAME]FUDGEM,\NATOM
	UNATS==UNATS+1

	VECTGO WHERE
	IFSN PNAME,,MAKAT [PNAME]TYPE,VALU,0
	IFSE PNAME,,MAKAT [NAME]TYPE,VALU,0
	VECRET
	]
	IFN FLAG,[VECTGO WHERE
	.GLOBAL $!NAME,%!NAME
	.LIFS -%!NAME
	$!NAME==.
	.ELDC
	IFSN PNAME,,MAKAT [PNAME]TYPE,VALU,1,%!NAME
	IFSE PNAME,,MAKAT [NAME]TYPE,VALU,1,%!NAME
	.LIFS %!NAME
	IFN <TYPE-TUNBOU>,[ZZZ==.
		LOC $!NAME
		TYPE,,0
		VALU
		LOC ZZZ
		]
	IFSN PNAME,,[ZZZ==.
		MACHAR [PNAME]NTYP,NVAL,1
		LOC $!NAME+4
		NTYP,,0
		NVAL
		LOC ZZZ
		]
	TVOFF==%!NAME
	.ELDC
	.LIFS -%!NAME
	%!NAME=TVOFF
	.ELDC
	VECRET
	]

	.TAG DONE
	NATOM==NATOM+1
	IFN FLAG,.LIFS $.
	SETADR \<NATOM-1>
	RADRES \NATOM
	IFN FLAG,.ELDC

TERMIN


DEFINE EXEC MAC,N,ARG
	MAC!N [ARG]
	TERMIN

DEFINE DEFMAC MAC,N,NAME,BODY,BDYARG
	DEFINE MAC!N ARG
	IFSE [ARG][NAME]BODY BDYARG
	TERMIN
TERMIN

DEFINE SETADR N
	%A!N==TVOFF
	TERMIN

DEFINE FUDGEM ARG
	TVOFF==%A!ARG
	.GO DONE
	TERMIN

DEFINE RADRES N
	DEFINE ADRES
	%A!N TERMIN
	TERMIN

;MACRO TO DEFINE A FUNCTION ATOM

DEFINE MFUNCTION NAME,TYPE,PNAME
	ADRES(TVP)
NAME":
	VECTGO DUMMY1
	CHATOM NAME,T!TYPE,DUMMY1,NAME,[PNAME]
	VECRET
	TERMIN

