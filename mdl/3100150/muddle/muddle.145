;MACROS TO PRINT VERSIONS OF PROGRAMS DURING ASSEMBLY

DEFINE NUMGEN SYM,\REST,N
	NN==NN-1
	N==<SYM_-30.>&77
	REST==<SYM_6>
	IFN N,IFGE <31-N>,IFGE <N-20>,TOTAL==TOTAL*10.+<N-20>
	IFN NN,NUMGEN REST
	EXPUNGE N,REST
	TERMIN

DEFINE VERSIO N
	PRINTC /VERSION = N
/
	TERMIN

TOTAL==0
NN==7

NUMGEN .FNAM2

IF1 [
RADIX 10.

