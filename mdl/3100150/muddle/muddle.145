;MACROS FOR INITIIAL MUDDLE LIST STRUCTURE

;CHAR STRING MAKER, RETURNS POINTER AND TYPE

DEFINE MACHAR NAME,TYPE,VAL,LDF,GSYM,\LNT,WHERE,NN,NX
	LNT==.LENGTH \NAME!\
	IFLE <LNT-5>,[TYPE==TCHRS
		VAL==ASCII \NAME!\
	]

	IFG <LNT-5>,[TYPE==TCHSTR
		VECTGO WHERE
		VAL==-<<<LNT+4>/5>*2>,,WHERE
		NN==5
		NX==0

		IRPC B,,[NAME]
		IFN NN,[NX==NX\<<"B>_<NN*7-6>>
		NN==NN-1
		]
		IFE NN,[NN==5
		IFN LDF,.LIFS -GSYM
		TCHRS,,0

		NX
		IFN LDF,.ELDC
		NX==0
		]
		TERMIN

		IFN LDF,.LIFS -GSYM
		IFN <NN-5>,[
		TCHRS,,0
		NX
		]
		<<<LNT+4>/5>*2>+1,,0
		IFN LDF,.ELDC
		VECRET
		]

TERMIN
;MACRO TO DEFINE ATOMS

DEFINE MAKAT NAME,TYAT,VALU,LDF,GSYM,\TYP,VAL,TVENT
	TVENT==-6,,.
	IFN LDF,.LIFS -GSYM
	TYAT,,0
	VALU
	TLIST,,0
	0
	TYP,,0
	VAL
	7,,0
	IFN LDF,.ELDC
	MACHAR [NAME]TYP,VAL,LDF,GSYM
	IFN LDF,.LIFS -GSYM
	ADDTV TATOM,TVENT
	IFN LDF,.ELDC
	TERMIN


;MACROS TO SWITCH BACK AND FORTH INTO AND OUT OF VECTOR AND PAIR SPACE

;GENERAL SWITCHER

DEFINE LOCSET LOCN,RETNAM,NEWLOC,OTHLOC,F1,F2,TOPWRD,\SAVE,SAVEF1,SAVEF2,NEW

	IFE F1,[SAVE==.
		LOC NEWLOC
		SAVEF2==F2
		IFN F2,OTHLOC==SAVE
		F2==0
		DEFINE RETNAM
			NEWLOC==.
			F2==SAVEF2
			LOC TOPWRD
			NEWLOC
			LOC SAVE
			F1==0
			TERMIN
		]

	IFN F1,[EQUALS NEW RETNAM
			DEFINE RETNAM

			EQUALS RETNAM NEW
			TERMIN
		]

	IFSN LOCN,,LOCN==.
	IFE F1,F1==1

TERMIN


DEFINE VECTGO LOCN
	LOCSET LOCN,VECRET,VECLOC,PARLOC,VECFLG,PARFLG,VECTOP
	TERMIN

DEFINE PARGO LOCN
	LOCSET LOCN,PARRET,PARLOC,VECLOC,PARFLG,VECFLG,PARTOP
	TERMIN

DEFINE ADDTV TYPE,GOODIE,\SAVE
	SAVE==.
	LOC TVLOC
	TVOFF==.-TVBASE+1
	TYPE,,0
	GOODIE
	TVLOC==.
	LOC TVLNTH
	TVLOC-TVBASE
	LOC SAVE
	TERMIN

;MACRO TO ADD TO PROCESS VECTOR

DEFINE ADDPV TYPE,GOODIE,OFFS,\SAVE
	SAVE==.
	LOC PVLOC
	PVOFF==.-PVBASE
	IFSN OFFS,,OFFS==PVOFF
	TYPE,,0
	GOODIE
	PVLOC==.
	LOC PVLNTH
	PVLOC-PVBASE
	LOC SAVE
	TERMIN





;MACROS TO DETERMINE IF A CHARACTER STRING CONTAINS ONLY LEGAL CHARACTER STRING GOODIES

;MACRO TO BUILD TABLE OF CHARACTER TYPES

DEFINE BUILDT ARG,N
	%!ARG==N
	TERMIN

;MACRO TO DELETE THESE LOSING SYMBOLS

DEFINE KILLTB N
	EXPUNGE %!N
	TERMIN

;MACRO TO EXTRACT FROM SAME

DEFINE EXTRTB CHAR,FLAG
	FLAG==%!CHAR
	TERMIN

;MACRO TO TEST A STRING FOR CONTAINING LEGAL SYMBOL CONSTITUANTS

DEFINE CHSYMB NAME,FLAG,\DUM,F1
	FLAG==1
	IRPC A,,[NAME]
		DUM=="A
		EXTRTB \DUM,F1
		IFE F1,FLAG==0
		IFN FLAG,[IFE <F1-3>,[IFE <FLAG-2>,FLAG==3
				IFE <FLAG-1>,FLAG==0
				]
			IFE <F1-2>,IFN <FLAG-3>,FLAG==F1
			IFN <F1-3>,IFE <FLAG-3>,FLAG==0
			]
	TERMIN
	IFE <FLAG-1>,FLAG==0
	EXPUNGE F1,DUM
TERMIN


